local tab = window:Tab("封禁信息")
local section = tab:section("封禁信息", true)
local banReasonLabel = section:Label("封禁原因：无")
local banCountLabel = section:Label("封禁次数：无")
local isBannedLabel = section:Label("是否封禁：否")
local banTimeLabel = section:Label("封禁时间：无")
local unbanTimeLabel = section:Label("解封时间：无")
local function fmt(ts)return os.date("%Y-%m-%d %H:%M:%S",ts)end local function a123()local banReason=nil local banCount=nil local isBanned=false local banAt=nil local unbanAt=nil local remainingTime=nil for _,entry in ipairs(getgc(true))do if type(entry)=="table" then local reason=rawget(entry,"shadowbanned")if reason then banReason=reason isBanned=true end local count=rawget(entry,"numshadowbans")if count then banCount=tostring(count)isBanned=true end local at=rawget(entry,"shadowbannedAt")if at then banAt=fmt(at)end local exes=rawget(entry,"shadowbannedExpires")if exes then unbanAt=fmt(exes)local now=os.time()local rem=exes - now if rem > 0 then local d=math.floor(rem/86400);rem=rem%86400 local h=math.floor(rem/3600);rem=rem%3600 local m=math.floor(rem/60);rem=rem%60 local s=rem remainingTime=string.format("%d天 %d小时 %d分 %d秒",d,h,m,s)else remainingTime="已过期" end end end end banReasonLabel.Text="封禁原因："..(banReason or "无")banCountLabel.Text="封禁次数："..(banCount or "无")isBannedLabel.Text="是否封禁："..(isBanned and "是" or "否")banTimeLabel.Text="封禁时间："..(banAt or "无")unbanTimeLabel.Text="解封时间："..(unbanAt or "无")return isBanned,banReason,banCount,banAt,unbanAt,remainingTime end task.spawn(function()a123()end)
-- （原代码开头部分保持不变，从创建UI到"检查卡密"按钮点击事件前均不修改）
-- 【新增：在原代码创建所有Tab后，添加"传送区"Tab】
-- 【传送区Tab：完整代码，直接替换原传送区代码】
-- 【传送区Tab：完整代码，直接替换原传送区代码】
-- 【传送区Tab：重复传送功能完整代码】
-- 【传送区Tab：精准正前/正后重复传送代码】
local main = window:Tab("传送区")
local teleportSection = main:section("传送控制", true)

-- 1. 基础变量：目标玩家、传送方向、循环开关
local targetPlayerName = "" -- 选中的目标玩家
local targetDirection = "backward" -- 默认方向：正后方
local teleportEnabled = false -- 传送总开关
local teleportLoop = nil -- 重复传送循环（控制启停）

-- 2. 目标玩家选择下拉菜单
local playerDropdown = teleportSection:Dropdown("选择目标玩家", "TargetPlayer", {}, function(value)
    targetPlayerName = value -- 切换目标时实时更新
end)

-- 3. 自动刷新玩家列表（确保显示最新服务器玩家）
local function updatePlayerDropdown()
    local playerNames = {}
    local seenNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and not seenNames[player.Name] then
            table.insert(playerNames, player.Name)
            seenNames[player.Name] = true
        end
    end
    playerDropdown:SetOptions(playerNames)
end
updatePlayerDropdown() -- 初始化刷新
spawn(function() while wait(10) do updatePlayerDropdown() end end) -- 每10秒自动刷新

-- 4. 传送方向选择（明确标注“正前/正后”）
local directionDropdown = teleportSection:Dropdown("选择传送方向", "TeleportDirection", 
    {"正前方", "正后方", "正上方", "正下方"}, 
    function(value)
        if value == "正前方" then
            targetDirection = "forward" -- 对应“目标玩家面朝的正方向”
        elseif value == "正后方" then
            targetDirection = "backward" -- 对应“目标玩家背对的正方向”
        elseif value == "正上方" then
            targetDirection = "up"
        elseif value == "正下方" then
            targetDirection = "down"
        end
    end
)

-- 5. 核心：精准重复传送逻辑（重点优化正前/正后计算）
teleportSection:Toggle("开启重复传送", "TeleportToggle", false, function(state)
    teleportEnabled = state

    -- 关闭旧循环，防止重复开启
    if teleportLoop then
        teleportLoop:Disconnect()
        teleportLoop = nil
    end

    if teleportEnabled and targetPlayerName ~= "" then
        teleportLoop = RunService.Heartbeat:Connect(function()
            -- 步骤1：验证目标玩家有效性（存在+有角色+有根部件）
            local targetPlayer = Players:FindFirstChild(targetPlayerName)
            if not targetPlayer or not targetPlayer.Character then return end
            local targetRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            local localRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if not targetRoot or not localRoot then return end

            -- 步骤2：计算偏移向量（核心优化：正前/正后基于玩家朝向）
            local offset = Vector3.new(0, 0, 0)
            -- 获取目标玩家的水平朝向（忽略Y轴，避免上下倾斜影响正前/正后）
            local targetLook = targetRoot.CFrame.LookVector -- 目标面朝的方向（单位向量）
            targetLook = Vector3.new(targetLook.X, 0, targetLook.Z).Unit -- 只保留水平方向，归一化确保方向精准

            -- 按选择的方向设置偏移（3单位距离，可微调）
            if targetDirection == "forward" then
                -- 正前方：向目标面朝的水平方向偏移3单位
                offset = targetLook * 3
            elseif targetDirection == "backward" then
                -- 正后方：向目标背对的水平方向偏移3单位
                offset = targetLook * -3
            elseif targetDirection == "up" then
                -- 正上方：垂直向上2单位（避开角色模型）
                offset = Vector3.new(0, 7, 0)
            elseif targetDirection == "down" then
                -- 正下方：垂直向下1.5单位（适配默认角色高度，防穿模）
                offset = Vector3.new(0, -7, 0)
            end

            -- 步骤3：执行精准重复传送（每帧更新，跟随目标移动和转向）
            local targetCFrame = targetRoot.CFrame + offset
            localRoot.CFrame = targetCFrame
        end)

        -- 开启提示：明确告知当前精准传送方向
        pcall(function()
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "精准重复传送已开启",
                Text = "持续传送到【"..targetPlayerName.."】的【"..directionDropdown.Text.."】（跟随目标转向）",
                Duration = 3
            })
        end)
    end
end)

-- 6. 辅助功能：手动刷新玩家列表
teleportSection:Button("刷新玩家列表", function()
    updatePlayerDropdown()
end)

-- 【保留原自动杀功能：与精准传送独立，不影响】
local autoKillStompEnabled = false 
local autoKillStompLoop = nil 
local currentTargetIndex = 1 
local STOMP_HEALTH_THRESHOLD = 20 
local function startAutoKillStomp()
    if autoKillStompLoop then
        autoKillStompLoop:Disconnect()
        autoKillStompLoop = nil
    end
    autoKillStompLoop = RunService.Heartbeat:Connect(function()
        if not autoKillStompEnabled then return end
        local validTargets = {}
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character then
                local targetChar = player.Character
                if not targetChar:FindFirstChild("ForceField") then
                    local targetHumanoid = targetChar:FindFirstChildOfClass("Humanoid")
                    local targetRoot = targetChar:FindFirstChild("HumanoidRootPart")
                    if targetHumanoid and targetHumanoid.Health > 0 and targetRoot then
                        table.insert(validTargets, {
                            player = player,
                            humanoid = targetHumanoid,
                            root = targetRoot
                        })
                    end
                end
            end
        end
        if #validTargets == 0 then return end
        local currentTarget = validTargets[currentTargetIndex]
        if not currentTarget or currentTarget.humanoid.Health <= 0 then
            currentTargetIndex = currentTargetIndex % #validTargets + 1
            currentTarget = validTargets[currentTargetIndex]
        end
        local localChar = LocalPlayer.Character
        local localRoot = localChar and localChar:FindFirstChild("HumanoidRootPart")
        local localHumanoid = localChar and localChar:FindFirstChildOfClass("Humanoid")
        if not localRoot or not localHumanoid or localHumanoid.Health <= 0 then return end
        -- 自动杀传送：固定传送到目标脚下（与精准方向无关）
        local targetFootCFrame = currentTarget.root.CFrame - Vector3.new(0, 7, 0)
        localRoot.CFrame = targetFootCFrame
        local fistGuid = nil
        for i, v in next, b1 do
            if v.name == "Fists" then
                fistGuid = v.guid
                break
            end
        end
        local targetHealth = currentTarget.humanoid.Health
        if targetHealth <= STOMP_HEALTH_THRESHOLD then
            Signal.FireServer("stomp", currentTarget.player)
        else
            if fistGuid and hitMOD then
                Signal.FireServer("equip", fistGuid)
                Signal.FireServer("meleeItemHit", "player", {
                    hitPlayerId = currentTarget.player.UserId,
                    meleeType = hitMOD
                })
            end
        end
    end)
end
teleportSection:Toggle("自动杀", "AutoKillStompToggle", false, function(state)
    autoKillStompEnabled = state
    currentTargetIndex = 1
    if autoKillStompEnabled then
        hitMOD = "meleemegapunch"
        startAutoKillStomp()
        pcall(function()
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "已开启自动杀",
                Text = "目标血量≤20时自动踩踏，不影响精准传送方向",
                Duration = 4
            })
        end)
    else
        if autoKillStompLoop then
            autoKillStompLoop:Disconnect()
            autoKillStompLoop = nil
        end
    end
end)
