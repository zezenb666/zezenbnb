        rootPart.CFrame = CFrame.new(174, 15, -142)
    elseif Value == '射线枪' then
        rootPart.CFrame = CFrame.new(159, -96, -528)
    elseif Value == '加特林' then
        rootPart.CFrame = CFrame.new(361, -2.6, -1453.19)
