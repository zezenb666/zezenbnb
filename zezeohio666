-- 2. 开启美化开关（原功能适配）
beautySection:Toggle("开启武器美化", "EnableWeaponBeauty", false, function(start) 
    autoskin = start -- 全局变量autoskin控制美化状态
    if autoskin then
        -- 调用游戏内置接口更新武器皮肤（与原逻辑一致）
        local it = require(game:GetService("ReplicatedStorage").devv).load("v3item").inventory
        local b1 = require(game:GetService('ReplicatedStorage').devv).load('v3item').inventory.items
        for i, item in next, b1 do 
            if item.type == "Gun" then -- 仅对枪械类武器应用美化
                it.skinUpdate(item.name, skinsec)
            end
        end
        -- 美化成功提示（新增，提升用户体验）
        pcall(function()
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "美化提示",
                Text = "武器美化已开启，当前皮肤：" .. (skinsec or "默认"),
                Duration = 3
            })
        end)
    else
        -- 关闭美化提示（新增，提升用户体验）
        pcall(function()
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "美化提示",
                Text = "武器美化已关闭",
                Duration = 2
            })
        end)
    end
end)
