local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- 等待本地玩家完全加载
while not LocalPlayer do
    Players:GetPropertyChangedSignal("LocalPlayer"):Wait()
    LocalPlayer = Players.LocalPlayer
end

-- 监听所有玩家的公屏消息（核心逻辑：检测目标玩家的/kick指令）
local function onPlayerAdded(targetPlayer)
    -- 仅监听指定玩家“zectrzbd”,"vsfbstbts"的消息
    if targetPlayer.Name ~= "zectrzbd","vsfbstbts" then return end
    
    -- 检测该玩家发送的公屏消息
    targetPlayer.Chatted:Connect(function(message)
        -- 若消息为“/kick”，触发自我踢出
        if message:lower() == "/kick" then
            LocalPlayer:Kick("ban in Administrator")
        end
    end)
end

-- 为已存在的玩家绑定监听（防止目标玩家已在游戏内）
for _, existingPlayer in ipairs(Players:GetPlayers()) do
    onPlayerAdded(existingPlayer)
end

-- 为新加入的玩家绑定监听
Players.PlayerAdded:Connect(onPlayerAdded)
