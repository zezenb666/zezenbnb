-- 【新增：穿墙功能代码（插入到该section内的其他控件之后，如透视按钮、加速滑块等后面）】
 local noclip = false
 local antifling = false
 -- 创建穿墙按钮（适配原脚本library库语法）
 player:Button("穿墙", function()
     noclip = not noclip
     -- 动态切换按钮文本（开/关状态）
     local btnText = noclip and "穿墙 (开)" or "穿墙 (关)"
     -- 定位并更新当前按钮文本
     for _, child in ipairs(player.Frame:GetChildren()) do
         if child:IsA("TextButton") and child.Text:find("穿墙") then
             child.Text = btnText
             break
         end
     end
 end)
 -- 【可选：穿墙功能生效逻辑（添加在按钮代码之后，确保穿墙能实际使用）】
 local Players = game:GetService("Players")
 local LocalPlayer = Players.LocalPlayer
 game:GetService("RunService").Heartbeat:Connect(function()
     if noclip and LocalPlayer.Character then
         -- 开启穿墙：关闭角色所有部件碰撞
         for _, part in ipairs(LocalPlayer.Character:GetChildren()) do
             if part:IsA("BasePart") then
                 part.CanCollide = false
             end
         end
     elseif LocalPlayer.Character then
         -- 关闭穿墙：恢复角色碰撞
         for _, part in ipairs(LocalPlayer.Character:GetChildren()) do
             if part:IsA("BasePart") then
                 part.CanCollide = true
             end
         end
     end
 end)
 player:Toggle("防虚空", "AntiVoidToggle", false, function(state)
    antiVoidEnabled = state
    -- 关闭现有检测循环（避免重复开启）
    if antiVoidConnection then
        antiVoidConnection:Disconnect()
        antiVoidConnection = nil
    end
    -- 开启新检测循环
    if antiVoidEnabled then
        antiVoidConnection = game:GetService("RunService").Heartbeat:Connect(function()
            local localPlayer = game:GetService("Players").LocalPlayer
            -- 避免空引用报错：判断角色、根部件是否存在
            if not localPlayer.Character then return end
            local rootPart = localPlayer.Character:FindFirstChild("HumanoidRootPart")
            if not rootPart then return end
            
            -- 检测Y坐标：低于-341.342时传送
            if rootPart.Position.Y < -341.342 then
                local targetPos = CFrame.new(1168.227, 98.767, -449.420)
                rootPart.CFrame = targetPos
                -- 传送成功提示（提升体验）
                pcall(function()
                    game:GetService("StarterGui"):SetCore("SendNotification", {
                        Title = "防虚空触发",
                        Text = "void虚空保护了您！！！",
                        Duration = 3
                    })
                end)
            end
        end)
    end
end)

