auto:Toggle("自动攻击ATM", "", false, function(v)
autoatm = v
if autoatm then
while autoatm and task.wait(1) do -- 每1秒检测一次，避免高频攻击
local player=game:GetService("Players").LocalPlayer 
local character=player.Character or player.CharacterAdded:Wait()
local rootPart=character:WaitForChild("HumanoidRootPart")
local devv = require(game:GetService("ReplicatedStorage").devv)
local item = devv.load("v3item")
local Signal = devv.load("Signal")

-- 1. 攻击可攻击的ATM（原逻辑保留，新增攻击后传送）
local attackedATM = false -- 标记是否攻击过ATM
for _, v in ipairs(workspace.Game.Props.ATM:GetChildren()) do
    if v:IsA("Model") and (v:GetAttribute("health") or 0) > 0 then
        if localPlayer.Character and localPlayer.Character:FindFirstChild("Humanoid") then
            local atmPos = v.WorldPivot.Position
            -- 传送至ATM位置并攻击
            rootPart.CFrame = CFrame.new(atmPos.x, atmPos.y, atmPos.z)
            task.wait(0.3) -- 等待角色到位
            
            -- 装备拳头
            local qtid = nil
            for i, invItem in next, item.inventory.items do
                if invItem.name == 'Fists' then
                    qtid = invItem.guid
                    break
                end
            end
            
            -- 攻击ATM
            if qtid then
                local ATMguid = v:GetAttribute("guid")
                Signal.FireServer("equip", qtid)
                Signal.FireServer("meleeItemHit", "prop", {meleeType = "meleepunch", guid = ATMguid})
                attackedATM = true -- 标记已攻击ATM
                break -- 只攻击一台ATM，避免同时攻击多台
            end
        end
    end
end

-- 2. 若攻击过ATM，传送至目标地点（XYZ：1653.06, 16.73, 531.20）
if attackedATM then
    task.wait(0.5) -- 等待攻击动作完成
    rootPart.CFrame = CFrame.new(1653.06, -16.73, -531.20) -- 传送至指定坐标
    -- 传送成功提示（可选，提升体验）
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "ATM攻击完成",
            Text = "已传送至指定地点（1653.06, 16.73, 531.20）",
            Duration = 2
        })
    end)
    attackedATM = false -- 重置标记，等待下次攻击
end

-- 3. 收集ATM掉落的现金（原逻辑保留）
for _,v in pairs(workspace.Game.Entities.CashBundle:GetDescendants()) do
    if v:IsA("ClickDetector") then
        local detectorPos = v.Parent:GetPivot().Position
        local distance = (rootPart.Position - detectorPos).Magnitude
        if distance <= 35 then
            fireclickdetector(v)
        end
    end
end
end
end
end)
