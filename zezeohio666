-- 先定义逃逸函数paolu2（需在Toggle前定义，避免回调时函数未声明）
local paolu2 = function()
    -- 循环内增加角色/人形/根部件判断，避免空引用报错
    while paolu1 do
        task.wait(0.1)
        local localPlayer = game:GetService('Players').LocalPlayer
        if not localPlayer.Character then continue end
        local humanoid = localPlayer.Character:FindFirstChild("Humanoid")
        local rootPart = localPlayer.Character:FindFirstChild("HumanoidRootPart")
        if not humanoid or not rootPart or humanoid.Health <= 0 then continue end

        -- 残血（≤35）时传送至指定位置
        if humanoid.Health <= 35 then
            local escapeCFrame = CFrame.new(175.191, 13.937, -132.69)
            rootPart.CFrame = escapeCFrame
            task.wait(20) -- 传送后冷却20秒
        end
    end
end

-- 残血自动逃逸Toggle（嵌入现有魔法窗口section）
zzzz:Toggle("残血自动逃逸", "paolu1", false, function(Value)
    paolu1 = Value
    if paolu1 then
        -- 用task.spawn开启函数，避免阻塞UI线程
        task.spawn(paolu2)
    end
end)
zzzz:Button("透视物品", function()
    -- 定义循环控制变量，避免重复开启循环
    if ItemESP_Loop then
        ItemESP_Loop:Disconnect()
    end
    -- 定义需要透视的物品名称列表，统一判断
    local targetItemNames = {
        "Military Armory Keycard", "Sawn Off", "Scar L", "Military Vest", "Raygun", 
        "UPS 45", "Medium Vest", "Deagle", "Glock 18", "Heavy Vest", "Diamond Ring", 
        "AS Val", "Money Printer", "Aug", "M4A1", "C4", "Stagecoach", "Diamond", 
        "Void Gem", "Dark Matter Gem", "Gold AK-47", "Barrett M107", "Gold Deagle", 
        "Double Barrel", "Dragunov", "RPK", "M249 SAW", "Flamethrower", 
        "Police Armory Keycard", "RPG", "Saiga 12", "Ammo Box"
    }
    -- 用Heartbeat循环替代while true+wait()，更贴合Roblox性能逻辑
    ItemESP_Loop = game:GetService("RunService").Heartbeat:Connect(function()
        local itemPickupFolder = game:GetService("Workspace").Game.Entities.ItemPickup
        if not itemPickupFolder then return end

        -- 1. 清除旧的BillboardGui（避免残留）
        for _, l in ipairs(itemPickupFolder:GetChildren()) do
            for _, k in ipairs(l:GetChildren()) do
                for _, billboard in ipairs(k:GetChildren()) do
                    if billboard:IsA("BillboardGui") then
                        billboard:Destroy()
                    end
                end
            end
        end

        -- 2. 为目标物品创建透视BillboardGui
        for _, l in ipairs(itemPickupFolder:GetChildren()) do
            for _, v in ipairs(l:GetChildren()) do
                -- 修复原逻辑中Class判断错误（需分别判断MeshPart和Part）
                if v:IsA("MeshPart") or v:IsA("Part") then
                    for _, e in ipairs(v:GetChildren()) do
                        if e:IsA("ProximityPrompt") then
                            -- 检查当前物品是否在目标列表中
                            local isTargetItem = false
                            for _, itemName in ipairs(targetItemNames) do
                                if e.ObjectText == itemName then
                                    isTargetItem = true
                                    break
                                end
                            end
                            if isTargetItem then
                                pcall(function()
                                    -- 创建BillboardGui
                                    local xd = Instance.new("BillboardGui")
                                    xd.Parent = v
                                    xd.AlwaysOnTop = true
                                    xd.Size = UDim2.new(0, 100, 0, 25)
                                    xd.StudsOffset = Vector3.new(0, 2, 0) -- 调整显示位置，避免遮挡物品

                                    -- 创建背景Frame
                                    local Frame = Instance.new("Frame")
                                    Frame.Parent = xd
                                    Frame.BackgroundColor3 = Color3.new(1, 1, 1)
                                    Frame.Size = UDim2.new(1, 0, 1, 0)
                                    Frame.BackgroundTransparency = 1

                                    -- 创建物品名称文本
                                    local text = Instance.new("TextLabel")
                                    text.Parent = Frame
                                    text.Text = e.ObjectText
                                    text.TextScaled = true
                                    text.Size = UDim2.new(1, 0, 1, 0)
                                    text.BackgroundTransparency = 1
                                    text.TextColor3 = Color3.new(1, 1, 1)
                                    text.TextStrokeColor3 = Color3.new(0, 0, 0) -- 加黑色描边，提升可读性
                                    text.TextStrokeTransparency = 0.3
                                end)
                            end
                        end
                    end
                end
            end
        end
    end)
end)
-- 无皮肤枪械无后+微射速+左键连发 按钮（嵌入现有魔法窗口section）
zzzz:Button("无皮肤枪械无后+微射速+左键连发", function()
    -- 定义循环控制变量，避免重复开启循环
    if GunModLoop then
        GunModLoop:Disconnect()
    end
    GunModLoop = game:GetService("RunService").Heartbeat:Connect(function()
        -- 枪械列表，统一遍历处理
        local gunNames = {
            "Raygun", "M1911", "Scar L", "Glock", "Mossberg", "RPG", "USP 45", 
            "Sawn Off", "Minigun", "Stagecoach", "Deagle", "RPK", "Glock 18", 
            "AK-47", "Tommy Gun", "M4A1", "Uzi", "MP7", "AUG", "Python"
        }
        local itemsFolder = game.ReplicatedStorage.Models.Items
        for _, gunName in ipairs(gunNames) do
            pcall(function()
                local gun = itemsFolder:FindFirstChild(gunName)
                if gun then
                    local muzzle = gun.Handle:FindFirstChild("Muzzle")
                    if muzzle then
                        local pointLight = muzzle:FindFirstChild("PointLight")
                        if pointLight and pointLight.Name == "PointLight" then
                            pointLight.Name = "PointLight1"
                        end
                    end
                end
            end)
        end
    end)
end)

